{
  "content_scripts": [{
    "js": ["script.js"],
    "matches": [
      "*://github.com/*"
    ]
  }],
  "description": "Makes the \"Add Comment\" button the first in GitHub pull request tab order",
  "icons": {
    "16": "assets/icon16.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  },
  "manifest_version": 2,
  "name": "GitHub PR Tab Fixer",
  "version": "1.0"
}
