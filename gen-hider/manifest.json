{
  "description": "Hides generated files from GitHub PRs",
  "background": {
    "persistent": false,
    "scripts": ["bg.js"]
  },
  "browser_action": {
    "default_title": "Hide generated files"
  },
  "icons": {
    "16": "assets/icon16.png",
    "19": "assets/icon19.png",
    "38": "assets/icon38.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  },
  "manifest_version": 2,
  "name": "GitHub PR gen hider",
  "permissions": ["activeTab"],
  "version": "1.0"
}
